{
  "name": "dashboard",
  "version": "0.4.7",
  "author": "Thomas Mays",
  "private": true,
  "engines": {
    "node": ">=v14.16",
    "npm": ">=7.7"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "md": "generate-md --layout github --input ./help-docs --output ./packages/renderer/help",
    "buildEnvTypes": "node scripts/buildEnvTypes.js",
    "build": "node scripts/build.js",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config electron-builder.config.js --dir",
    "pretest": "cross-env MODE=test npm run build",
    "test": "node tests/app.spec.js",
    "watch": "node scripts/watch.js",
    "fix-vue": "node scripts/fixVueDevtools.js",
    "lint": "eslint . --ext js,ts,vue",
    "pretypecheck": "npm run buildEnvTypes",
    "typecheck-main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck-preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck-renderer": "vue-tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "npm run typecheck-main && npm run typecheck-preload && npm run typecheck-renderer",
    "postinstall": "electron-builder install-app-deps",
    "reset": "node scripts/reset.js"
  },
  "browserslist": [
    "Chrome 89"
  ],
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --cache --fix"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.0.15",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fs-extra": "^9.0.11",
    "@types/mathjs": "^6.0.12",
    "@types/numeral": "^2.0.1",
    "@types/string-similarity": "^4.0.0",
    "@types/tabulator-tables": "^4.9.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "cross-env": "^7.0.3",
    "electron": "^12.0.6",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^2.3.5",
    "eslint": "^7.25.0",
    "eslint-plugin-vue": "^7.9.0",
    "feather-icons": "^4.28.0",
    "lint-staged": "^10.5.4",
    "markdown-styles": "^3.2.0",
    "simple-git-hooks": "^2.4.1",
    "spectron": "^14.0.0",
    "typescript": "^4.2.4",
    "vite": "^2.2.3",
    "vite-svg-loader": "^1.5.1",
    "vue-devtools": "^5.1.4",
    "vue-tsc": "^0.0.26"
  },
  "dependencies": {
    "@electron/remote": "^1.1.0",
    "bootstrap": "^4.6.0",
    "chalk": "^4.1.1",
    "chart.js": "^3.2.1",
    "chokidar": "^3.5.1",
    "date-fns": "^2.21.2",
    "dotenv": "^8.2.0",
    "electron-log": "^4.3.5",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.3.8",
    "fs-extra": "^8.1.0",
    "jquery": "^3.6.0",
    "mathjs": "^9.3.2",
    "numeral": "^2.0.6",
    "popper.js": "^1.16.1",
    "sass": "^1.32.12",
    "simple-statistics": "^7.7.0",
    "string-similarity": "^4.0.4",
    "tabulator-tables": "^4.9.3",
    "vue": "^3.0.11",
    "vue-router": "^4.0.6",
    "xlsx": "^0.16.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/parsehex/dashboard2.git"
  }
}
